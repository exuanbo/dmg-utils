{
  "name": "dmg-utils",
  "version": "0.0.1",
  "description": "Util lib for DMG files.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rimraf dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist/types",
    "lint": "eslint \"**/*.{js,ts}\"",
